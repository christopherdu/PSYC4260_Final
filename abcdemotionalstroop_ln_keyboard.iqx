	list.accuracy.reset();
	list.latencies.reset();
	values.practice1Runs += 1;
]
/ trials = [1 = getReady1; 2-9 = practice1]
/ branch = [
	if (list.accuracy.mean < 0.75 && !monkey.monkeymode){
		return block.repeatPractice;
	};
]
</block>


